{
  "CallerReference": "tee-rex-prod-2026-02-11",
  "Comment": "tee-rex production â€” app (S3) + prover/TEE (EC2)",
  "DefaultRootObject": "index.html",
  "HttpVersion": "http2",
  "PriceClass": "PriceClass_100",
  "Enabled": true,
  "Origins": {
    "Quantity": 3,
    "Items": [
      {
        "Id": "s3-app",
