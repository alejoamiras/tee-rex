          "CachedMethods": { "Quantity": 2, "Items": ["GET", "HEAD"] }
        },
        "FunctionAssociations": {
          "Quantity": 1,
          "Items": [
            {
              "FunctionARN": "arn:aws:cloudfront::<ACCOUNT_ID>:function/tee-rex-strip-prefix",
