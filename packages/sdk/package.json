{
  "name": "@alejoamiras/tee-rex",
  "version": "0.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/alejoamiras/tee-rex",
    "directory": "packages/sdk"
  },
  "exports": "./src/index.ts",
  "publishConfig": {
    "exports": {
      ".": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    },
    "access": "public"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "dev": "tsc -w",
    "build": "rm -rf dist && tsc",
    "test:unit": "bun test src/",
    "test:e2e": "bun test --preload ./e2e/e2e-setup.ts e2e/",
    "test:e2e:nextnet": "AZTEC_NODE_URL=https://nextnet.aztec-labs.com bun test --preload ./e2e/e2e-setup.ts e2e/nextnet.test.ts",
    "test:lint": "tsc --noEmit --emitDeclarationOnly false",
    "prepublishOnly": "bun run test:lint && bun run build"
  },
  "dependencies": {
    "@aztec/bb-prover": "5.0.0-nightly.20260220",
    "@aztec/foundation": "5.0.0-nightly.20260220",
    "@aztec/noir-acvm_js": "5.0.0-nightly.20260220",
    "@aztec/noir-noirc_abi": "5.0.0-nightly.20260220",
    "@aztec/stdlib": "5.0.0-nightly.20260220",
    "@logtape/logtape": "^2.0",
    "@peculiar/x509": "^1.12.0",
    "cbor-x": "^1.6.0",
    "ky": "^1.14.3",
    "ms": "^2.1.3",
    "openpgp": "6.3.0",
    "ox": "^0.12.1",
    "ts-essentials": "^10.1.1",
    "ufo": "^1.6.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@aztec/accounts": "5.0.0-nightly.20260220",
    "@aztec/aztec.js": "5.0.0-nightly.20260220",
    "@aztec/noir-contracts.js": "5.0.0-nightly.20260220",
    "@aztec/pxe": "5.0.0-nightly.20260220",
    "@aztec/simulator": "5.0.0-nightly.20260220",
    "@aztec/wallets": "5.0.0-nightly.20260220",
    "@types/ms": "^2.1.0"
  }
}
